{
    "eslint.autoFixOnSave": false,
    "eslint.enable": true,
    "eslint.packageManager": "yarn",
    "eslint.provideLintTask": false,
    "files.exclude": {
        "node_modules": true
    },
    "flow.useNPMPackagedFlow": true,
    "javascript.validate.enable": false,
    "prettier.requireConfig": true,
    "search.exclude": {
        ".cache": true,
        ".vscode": true,
        "**/node_modules": true,
        "commonjs": true,
        "coverage": true,
        "dist": true,
        "es": true,
        "flow-typed": true,
        "i18n": true,
        "reports": true
    },
    "typescript.validate.enable": false
}
